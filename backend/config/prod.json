{
    "server": {
        "port": 8080,
        "host": "localhost"
    },
    "token": {
        "expire": {
            "access": "1h",
            "refresh": "7d"
        }
    },
    "featureFlags": {
        "routes": {
            "dish": {
                "enabled": true
            },
            "transactions": {
                "enabled": true
            }
        }
    },
    "collections": {
        "transactions": "transactions-new",
        "dishes": "dishes",
        "users": "users",
        "qrcodes": "qr-codes",
        "cron": "cron",
        "email": "email"
    },
    "cron": {
        "default": {
            "email": "* * * * * Wednesday"
        }
    },
    "aws": {
        "region": "us-west-2"
    }
}